<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.com.scitc.demo7.model.Imagesaddress" table="imagesaddress" schema="student">
        <id name="id">
            <column name="id" sql-type="int(10)"/>
        </id>
        <property name="images">
            <column name="images" sql-type="varchar(2000)" length="2000"/>
        </property>
        <property name="address">
            <column name="address" sql-type="varchar(200)" length="200"/>
        </property>
    </class>
</hibernate-mapping>