<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.com.scitc.demo7.model.Proorder" table="proorder" schema="student">
        <id name="id">
            <column name="id" sql-type="int(10)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="product">
            <column name="product" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="price">
            <column name="price" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="date">
            <column name="date" sql-type="datetime"/>
        </property>
    </class>
</hibernate-mapping>